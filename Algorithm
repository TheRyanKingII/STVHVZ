int main()
{
  int numCands;
  HashMap<string,int> candidates;
  for(int i = 0; i < numCands; i++)
    addCandidates(candidates);
  
}
void addCandidates(HashMap<string,int> & map)
{
  string n;
  cout << "Enter candidate's name: ";
  cin >> n;
  map[n] = 0;
}
